
pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'these is building stage'
            }
        }
         stage('test') {
            steps {
                echo 'these is these is a testing stage'
            }
        }
        stage('jenkins') {
            steps {
                echo 'these is these is a jenkins filetage'
            }
        }
    }
}
